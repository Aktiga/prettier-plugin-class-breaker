{
	"name": "@aktiga/prettier-plugin-class-breaker",
	"version": "1.0.0",
	"description": "A fork of Francis Gregori's Prettier Plugin of the same name",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"lint": "eslint . --ext .ts",
		"test": "echo \"Error: no test specified\" && exit 1",
		"check": "tsc && prettier -c . && npm run lint",
		"format": "prettier -w .",
		"reinstall": "bash ./src/scripts/deleteNodeModules.sh && pnpm install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Aktiga/prettier-plugin-class-breaker.git"
	},
	"keywords": [
		"prettier",
		"prettier-plugin",
		"jsx",
		"css-classes",
		"formatting",
		"typescript",
		"react",
		"code-style",
		"code-readability",
		"linting",
		"multiline",
		"multi-line",
		"classes",
		"classnames"
	],
	"author": "Keaton Bishop-Marx",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@babel/generator": "^7.23.4",
		"@babel/parser": "^7.23.4",
		"@babel/traverse": "^7.23.4",
		"@babel/types": "^7.23.4",
		"@types/babel__generator": "^7.6.7",
		"@types/babel__traverse": "^7.20.4",
		"@types/node": "^20.9.3",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.0",
		"prettier-plugin-merge": "^0.7.0",
		"prettier-plugin-multiline-arrays": "^3.0.6",
		"prettier-plugin-organize-imports": "^4.0.0",
		"strip-indent": "^4.0.0",
		"typescript": "^4.9.5"
	}
}
